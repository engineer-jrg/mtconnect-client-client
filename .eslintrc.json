{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "react/jsx-props-no-spreading": [ "error", {
            //"html" : "enforce",
            "custom": "ignore" 
            // "excepciones" : [ "RouteWithSubRoutes" ]
        }],
        "camelcase": ["error", {"allow": ["^UNSAFE_"]}],
        "linebreak-style": ["error", "windows"]
    },
    "settings": {
        "import/resolver": "webpack" 
    }
}